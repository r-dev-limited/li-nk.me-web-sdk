{
  "name": "@li-nk.me/web-sdk",
  "version": "0.2.0",
  "description": "Zero-dependency browser SDK for LinkMe deep and deferred links.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "vitest run"
  },
  "keywords": [
    "linkme",
    "deep-linking",
    "deferred",
    "web",
    "sdk"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}